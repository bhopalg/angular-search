{
  "name": "angular-search",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "tslint -p .",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma es5 --write \"**/*.ts\"",
    "e2e": "ng e2e",
    "test": "jest",
    "clean": "git clean -xdf && yarn"
  },
  "engines": {
    "node": ">=8.6.0",
    "yarn": ">=1.3.0 <2.0.0"
  },
  "keywords": [
    "angular",
    "rxjs"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "ramda": "^0.26.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "^3.4.4",
    "@types/jest": "^24.0.21",
    "@types/mockdate": "^2.0.0",
    "@types/node": "~8.9.4",
    "@types/ramda": "^0.26.33",
    "@types/rimraf": "^2.0.3",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-preset-angular": "^8.0.0",
    "jest-zone-patch": "^0.0.10",
    "mockdate": "^2.0.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.4.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(ts)\""
    }
